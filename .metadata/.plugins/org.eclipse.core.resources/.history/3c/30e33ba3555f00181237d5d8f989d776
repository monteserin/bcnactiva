package com.pablomonteserin.siniestros;

public class VehiculoSiniestro{

	private Vehiculo v;
	private int perdida;
	public Vehiculo getV() {
		return v;
	}
	public void setV(Vehiculo v) {
		this.v = v;
	}
	public int getPerdida() {
		return perdida;
	}
	public void setPerdida(int perdida) {
		this.perdida = perdida;
	}
	
	
}

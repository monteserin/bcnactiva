package com.pablomonteserin.comparacion;

public class Main {

	public static void main(String[] args) {
		String txt1 = "ba";
		String txt2 = "bb";
		
		
		if(txt1.compareTo(txt2)<0) {
			System.out.println("txt1 es menor que txt2");
		}else if(txt1.compareTo(txt2)>0) {
			System.out.println("txt2 es mayor que txt1");
		}

	}

}
